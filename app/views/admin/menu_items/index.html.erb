<div class="col-md-9">
  <h1>Menu Items</h1>
  <a href="<%= new_admin_menu_item_path %>">Add a new item</a>
  <table>
    <% @menu_items.each do |item| %>
      <tr>
        <td>
          <%= link_to(item.name, edit_admin_menu_item_path(item)) %>
        </td>
        <td>
          <%= link_to('Delete this shizz', admin_menu_item_path(item), method: :delete, confirm: 'Yeah?!') %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
